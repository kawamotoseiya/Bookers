<h2>Users</h2>
<table>
	<thead>
		<tr>
			<th>name</th>
			<th colspan="1"></th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
			<tr>
				<td><%= user.name %></td>
				<td><%= link_to "Show", user_path(user) %></td>
			</tr>
		<% end %>
	</tbody>
</table>
<%= render 'books/list', books:@books, user:current_user %>